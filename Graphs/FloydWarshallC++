#include <bits/stdc++.h>
using namespace std;
const int INF=1e9;
int main()
{
    vector<vector<int>> graph= { { 0, 5, INF, 10 },
                        { INF, 0, 3, INF },
                        { INF, INF, 0, 1 },
                        { INF, INF, INF, 0 } };
    int n=graph.size();
    vector<vector<int>> dist=graph;
    //Here K will act as intermediate to check for the shortest path.Where we will loop through all the possible values .
    for(int k=0;k<n;k++)
    {
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(dist[i][k]+dist[k][j]<dist[i][j])
                {
                    dist[i][j]=dist[k][j]+dist[i][k];
                }
            }
        }
    }
            for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(dist[i][j]==INF)
                {
                    cout<<"INF ";
                }
                else
                    cout<<dist[i][j]<<" ";
            }
            cout<<endl;}

}
